<?xml version="1.0"?>
<!DOCTYPE hook PUBLIC "-//Liferay//DTD Hook 7.4.0//EN" "http://www.liferay.com/dtd/liferay-hook_7_4_0.dtd">

<hook>
	<portal-properties>portal.properties</portal-properties>
	<language-properties>
		content/Language_en.properties
	</language-properties>
	<custom-jsp-dir>/WEB-INF/jsp</custom-jsp-dir>
	<servlet-filter>
		<servlet-filter-name>FilterHook</servlet-filter-name>
		<servlet-filter-impl>com.liferay.hook.FilterHook</servlet-filter-impl>
	</servlet-filter>
	<servlet-filter-mapping>
		<servlet-filter-name>FilterHook</servlet-filter-name>
		<url-pattern>/web/demo/*</url-pattern>

	</servlet-filter-mapping>


</hook>

